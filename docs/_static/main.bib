@article{jiao_2017,
title = {Improved maize reference genome with single-molecule technologies.},
author = {Jiao, Yinping and Peluso, Paul and Shi, Jinghua and Liang, Tiffany and Stitzer, Michelle C and Wang, Bo and Campbell, Michael S and Stein, Joshua C and Wei, Xuehong and Chin, Chen-Shan and Guill, Katherine and Regulski, Michael and Kumari, Sunita and Olson, Andrew and Gent, Jonathan and Schneider, Kevin L and Wolfgruber, Thomas K and May, Michael R and Springer, Nathan M and Antoniou, Eric and {McCombie}, W Richard and Presting, Gernot G and {McMullen}, Michael and Ross-Ibarra, Jeffrey and Dawe, R Kelly and Hastie, Alex and Rank, David R and Ware, Doreen},
pages = {524-527},
url = {http://dx.doi.org/10.1038/nature22971},
year = {2017},
