title = {{PANNZER}: high-throughput functional annotation of uncharacterized proteins in an error-prone environment.},
author = {Koskinen, Patrik and Törönen, Petri and Nokso-Koivisto, Jussi and Holm, Liisa},
pages = {1544-1552},
url = {http://dx.doi.org/10.1093/bioinformatics/btu851},
